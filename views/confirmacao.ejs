    </table>
    <div class="h6 text-center">SUAS RESPOSTAS FORAM ENVIADAS</div>
    <a href="/" class="btn btn-primary">VOLTAR PARA O ÍNICIO</a>
    </div> 
       
</body>
<script src="./js/bootstrap.min.js"></script>
<script src="./js/popper.min.js"></script>
